#include <stdio.h>
int missing(int arr[], int n) {
    int total = (n * (n+1)) / 2;
    int sum = 0;
    for (int i = 0; i < n-1; i++) {
        sum += arr[i];
    }
    return total - sum;
}

int main() {
    int arr[] = {1, 2, 3, 4,5,6,7,9};
    int n = sizeof(arr)/sizeof(arr[0]); // numbers from 1 to 5
    printf("Missing number is %d\n", missing(arr, n));
    return 0;
}

