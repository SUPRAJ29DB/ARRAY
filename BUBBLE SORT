#include <stdio.h>
int main() {
    int n, i, j, temp;
    printf("Enter size: ");
    scanf("%d", &n);
    int arr[n];
    printf("Enter elements: ");
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    // Bubble Sort (ascending order)
    for (i = 0; i < n; i++) {
        for (j = 0; j < n - i - 1; j++) {
            if (arr[j] > arr[j + 1]) {  // Correct comparison
                temp = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = temp;
            }
        }
    }
    printf("Sorted array is: ");
    for (i = 0; i < n; i++) {
        printf("%d\t", arr[i]);
    }
    return 0;
}







